{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "build": "electron-packager . LabelIF --out build",
    "build_win": "electron-packager . LabelIF --out build --platform win32 ",
    "build_mac": "electron-packager . LabelIF --out build --platform darwin "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-packager": "^10.1.2"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "fabric": "^1.7.22",
    "fs-extra": "^5.0.0",
    "xml2js": "^0.4.19"
  }
}
